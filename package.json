{
  "name": "@photo/backend",
  "version": "1.0.0",
  "description": "Photo backend application",
  "engines": {
    "node": "~12.18.2",
    "npm": "6.14.5"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "compile": "tsc --project tsconfig.json",
    "compile:watch": "npm run compile -- --watch",
    "compile-tests": "tsc --p ./test",
    "lint": "eslint '{src,test}/**/*.ts' --max-warnings=0",
    "lint:fix": "eslint '{src,test}/**/*.ts' --fix",
    "premigrate": "npm run compile",
    "seed": "npm run sequelize -- db:seed:all",
    "migrate": "npm run sequelize -- db:migrate",
    "rollback": "npm run sequelize -- db:migrate:undo",
    "migrations:create": "npm run sequelize -- migration:generate --migrations-path ./src/migrations",
    "postmigrations:create": "ls ./src/migrations/*.js | xargs -I {} sh -c 'mv $1 ./src/migrations/`basename $1 .js`.ts' - {}",
    "seeders:create": "npm run sequelize -- seed:generate --seeders-path ./src/seeders",
    "postseeders:create": "ls ./src/seeders/*.js | xargs -I {} sh -c 'mv $1 ./src/seeders/`basename $1 .js`.ts' - {}",
    "sequelize": "sequelize",
    "dev": "concurrently 'npm run compile:watch' 'npm run start:watch'",
    "start": "node  --require source-map-support/register ./dist/src/app.js",
    "start:log": "export $(grep -v '^#' .env | xargs) && node  --require source-map-support/register ./dist/src/app.js | pino-pg --connectionUrl ${DATABASE_URL} --table ${PINO_TABLE} --column ${PINO_COLUMN}",
    "start:watch": "nodemon -w dist -e js --exec 'npm run start'",
    "pretest": "npm run compile",
    "test": "npm run mocha",
    "coverage": "NODE_ENV=test nyc mocha --exit --require source-map-support/register --require ./dist/test/init.js --recursive ./dist/test --timeout 30000",
    "mocha": "NODE_ENV=test mocha --exit --require source-map-support/register --require ./dist/test/init.js --recursive ./dist/test --timeout 30000"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.19.0",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "datauri": "^2.0.0",
    "dotenv-safe": "^5.0.1",
    "express": "^4.16.3",
    "express-formidable": "^1.2.0",
    "express-pino-logger": "^5.0.0",
    "express-session": "^1.17.0",
    "helmet": "^3.13.0",
    "md5": "^2.2.1",
    "multer": "^1.4.2",
    "node-forge": "^0.10.0",
    "pg": "^7.4.3",
    "pino": "^6.4.1",
    "pino-pg": "^2.2.0",
    "ramda": "^0.26.1",
    "request-promise": "^4.2.5",
    "sequelize": "^4.44.4",
    "typescript-json-validator": "^2.4.2",
    "uuid": "^8.3.0",
    "walk-sync": "^0.3.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.5",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-datetime": "^0.0.32",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.8",
    "@types/dotenv-safe": "^8.1.1",
    "@types/express": "^4.16.0",
    "@types/express-formidable": "^1.0.4",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-session": "^1.15.16",
    "@types/helmet": "^0.0.41",
    "@types/is-ci": "^2.0.0",
    "@types/md5": "^2.2.0",
    "@types/mocha": "^5.2.5",
    "@types/mock-req-res": "^1.1.2",
    "@types/multer": "^1.4.0",
    "@types/node": "^8.10.29",
    "@types/node-forge": "^0.9.5",
    "@types/pino": "^6.3.0",
    "@types/ramda": "^0.26.8",
    "@types/redis": "^2.8.16",
    "@types/request-promise": "^4.1.45",
    "@types/sequelize": "^4.27.25",
    "@types/uuid": "^8.0.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-datetime": "^1.5.0",
    "check-code-coverage": "^1.9.3",
    "concurrently": "^4.1.0",
    "eslint": "^7.7.0",
    "eslint-plugin-import": "^2.22.0",
    "factory-girl": "^5.0.2",
    "mocha": "^5.2.0",
    "mock-req-res": "^1.2.0",
    "nodemon": "^1.19.0",
    "nyc": "^15.1.0",
    "pino-pretty": "^4.1.0",
    "request": "^2.88.2",
    "sequelize-cli": "^5.5.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  },
  "license": "ISC",
  "private": true
}
